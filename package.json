{
  "name": "keep",
  "version": "1.0.0",
  "description": "A todo list app",
  "main": "index.js",
  "scripts": {
    "test": "mocha  -r esm test/**/*.test.js",
    "test-watch": "nodemon --exec npm test",
    "start": "nodemon -r esm",
    "migrate": "db-migrate --migrations-dir ./app/db/migrations",
    "migrate:create": "npm run migrate create",
    "migrate:up": "npm run migrate up",
    "migrate:down": "npm run migrate down",
    "migrate:reset": "npm run migrate reset",
    "seed:up": "npm run migrate up:seeds",
    "seed:down": "npm run migrate down:seeds",
    "seed:reset": "npm run migrate reset:seeds",
    "lint": "eslint --fix --ext .js ./"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Aijeyomah/keep.git"
  },
  "author": "Aijey_omah",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Aijeyomah/keep/issues"
  },
  "homepage": "https://github.com/Aijeyomah/keep#readme",
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "app-root-path": "^3.0.0",
    "bcrypt": "^5.0.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "eslint-plugin-import": "^2.22.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "kue": "^0.11.6",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.4",
    "pg": "^8.3.0",
    "pg-promise": "^10.5.8",
    "redis": "^3.0.2",
    "socket.io": "^2.4.0",
    "uuid": "^8.2.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "db-migrate": "^0.11.11",
    "db-migrate-pg": "^1.2.2",
    "eslint": "^7.4.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "expect": "^26.2.0",
    "mocha": "^8.0.1",
    "supertest": "^4.0.2"
  }
}
